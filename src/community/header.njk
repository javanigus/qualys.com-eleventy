<div class="q-header__background">
	{% include "src/_includes/header-ueno.njk" %}
</div>
<script>
	/*
	 * trigger an event when loaded so Salesforce knows when the header has loaded
	 */
	(function () {

		var event;

		if (window.CustomEvent) {

			event = new CustomEvent("QualysHeaderLoaded");

		} else {

			event = document.createEvent("CustomEvent");
			event.initCustomEvent("QualysHeaderLoaded", true, true);

		}

		document.dispatchEvent(event);

	})();
</script>
