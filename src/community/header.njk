		var event;

		if (window.CustomEvent) {

			event = new CustomEvent("QualysHeaderLoaded");

		} else {

			event = document.createEvent("CustomEvent");
			event.initCustomEvent("QualysHeaderLoaded", true, true);

		}

		document.dispatchEvent(event);

	})();
</script>
