<div id="search" class="CoveoSearchInterface" data-enable-history="true" data-design="new">
  <div class="CoveoFoldingForThread" data-field="@foldingcollection" data-child="@foldingchild" data-parent="@foldingparent"></div>
  <div class="CoveoAnalytics" data-search-hub="CommunitySearch"></div>
  <h1 class="q-coveo-title">
    Search Results
  </h1>
  <div class="coveo-main-section">
    <div class="coveo-facet-column">
      <div class="CoveoFacet" data-title="Cloud Apps" data-field="@cloudapps" data-is-multi-value-field="true" data-number-of-values="10"></div>
      <div class="CoveoFacet" data-title="Source" data-field="@source" data-number-of-values="" data-value-caption="{&quot;KnowledgeArticles&quot;:&quot;Knowledge Base&quot;}"></div>
      <div class="CoveoFacet" data-title="Content Type" data-field="@contenttype" data-number-of-values="" data-value-caption="{}"></div>
      <div class="CoveoFacet" data-title="Author" data-field="@author" data-tab="Discussions, Blog"></div>
      <div class="CoveoFacet" data-title="Tags" data-field="@tags" data-is-multi-value-field="true" data-number-of-values="3" data-tab="Discussions, Blog"></div>
      <div class="CoveoFacetSlider" data-title="Date Range" data-field="@date" data-date-field="true" data-graph-steps="30" data-graph-animation-duration="" data-steps="" data-range-slider="true" data-date-format="MMM, yyyy"></div>
    </div>
    <div class="coveo-results-column">
      <div class="coveo-tab-section">
        <div class="CoveoTab" data-id="All" data-caption="All Content"></div>
        <div class="CoveoTab" data-id="Discussions" data-caption="Discussions" data-expression="@source=(Discussions,DiscussionsQA)"></div>
        <div class="CoveoTab" data-id="Blog" data-caption="Blog" data-expression="@source=(Blog,Notifications,Webinars)"></div>
        <div class="CoveoTab" data-id="Training" data-caption="Training" data-expression="@source=(Training,Vimeo)"></div>
        <div class="CoveoTab" data-id="Docs" data-caption="Docs" data-expression="@source=(&quot;Product Help&quot;, &quot;Product Docs&quot;, &quot;Release Notes&quot;)"></div>
        <div class="CoveoTab" data-id="Support" data-caption="Support" data-expression="@source=(KnowledgeArticles)"></div>
      </div>
      <div class="CoveoShareQuery"></div>
      <div class="CoveoPreferencesPanel">
        <div class="CoveoResultsPreferences"></div>
        <div class="CoveoResultsFiltersPreferences"></div>
      </div>
      <div class="CoveoTriggers"></div>
      <div class="CoveoBreadcrumb"></div>
      <div class="CoveoDidYouMean"></div>
      <div class="coveo-results-header">
        <div class="coveo-summary-section">
          <span class="CoveoQuerySummary">
            <div class="coveo-show-if-no-results"></div>
          </span>
          <span class="CoveoQueryDuration"></span>
        </div>
        <div class="coveo-result-layout-section">
          <span class="CoveoResultLayout"></span>
        </div>
        <div class="coveo-sort-section">
          <span class="CoveoSort" data-sort-criteria="relevancy" data-caption="Relevance"></span>
          <span class="CoveoSort" data-sort-criteria="date descending,date ascending" data-caption="Date"></span>
        </div>
      </div>
      <div class="CoveoHiddenQuery"></div>
      <div class="CoveoErrorReport" data-pop-up="false"></div>
      <div class="CoveoResultList" data-layout="list" data-wait-animation="fade" data-auto-select-fields-to-include="true">
        <script id="KnowledgeResultTemplate" type="text/html" class="result-template" data-layout="list" data-field-objecttype="Knowledge">
          <div class="coveo-result-frame">
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <span class="CoveoFieldValue q-content-type" data-field="@contenttype"></span>
              </div>
            </div>
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <a class="CoveoResultLink" data-href-template="https://success-qa.qualys.com/discussions/s/article/${raw.sfurlname}"></a>
                  </div>
                  <div class="coveo-result-cell quickview">
                    <div class="CoveoQuickview coveo-accessible-button" data-tooltip-placement="bottom-end" role="button" aria-label="Quick View" tabindex="0">
                      <button>Quick View</button>
                    </div>
                  </div>
                </div>
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <span class="CoveoFieldValue" data-field="@date" data-helper="date"></span>
                    <span class="CoveoText" data-value="–"></span>
                    <span class="CoveoExcerpt"></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </script>
        <script id="FoldingParentResultTemplate" type="text/html" class="result-template" data-layout="list" data-field-foldingchild="">
          <div class="coveo-result-frame">
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <span class="CoveoFieldValue q-content-type" data-field="@contenttype"></span>
              </div>
            </div>
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <a class="CoveoResultLink"></a>
                  </div>
                  <div class="coveo-result-cell quickview">
                    <div class="CoveoQuickview coveo-accessible-button" data-tooltip-placement="bottom-end" role="button" aria-label="Quick View" tabindex="0">
                      <button>Quick View</button>
                    </div>
                  </div>
                </div>
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <span class="CoveoFieldValue" data-field="@date" data-helper="date"></span>
                    <span class="CoveoText" data-value="–"></span>
                    <span class="CoveoExcerpt"></span>
                  </div>
                </div>
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <div class="q-js-fields-for-result-folding hidden">
                      <span class="CoveoFieldValue" data-field="@jivereplycount"></span>
                      <span class="CoveoFieldValue" data-field="@wpcommentcount"></span>
                    </div>
                    <span class="CoveoResultFolding" data-result-template-id="FoldingChildResultTemplate"></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </script>
        <script id="FoldingChildResultTemplate" type="text/html" class="result-template" data-layout="list" data-field-foldingparent="">
          <div class="coveo-result-frame">
            <div class="coveo-result-row" style="padding: 15px 0 15px 30px">
              <div class="coveo-result-cell">
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <a class="CoveoResultLink"></a>
                  </div>
                </div>
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <span class="CoveoFieldValue" data-field="@date" data-helper="date"></span>
                    <span class="CoveoText" data-value="–"></span>
                    <span class="CoveoExcerpt"></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </script>
        <script id="AllOthersResultTemplate" type="text/html" class="result-template" data-layout="list">
          <div class="coveo-result-frame">
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <span class="CoveoFieldValue q-content-type" data-field="@contenttype"></span>
              </div>
            </div>
            <div class="coveo-result-row">
              <div class="coveo-result-cell">
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <a class="CoveoResultLink"></a>
                  </div>
                  <div class="coveo-result-cell quickview">
                    <div class="CoveoQuickview coveo-accessible-button" data-tooltip-placement="bottom-end" role="button" aria-label="Quick View" tabindex="0">
                      <button>Quick View</button>
                    </div>
                  </div>
                </div>
                <div class="coveo-result-row">
                  <div class="coveo-result-cell">
                    <span class="CoveoFieldValue" data-field="@date" data-helper="date"></span>
                    <span class="CoveoText" data-value="–"></span>
                    <span class="CoveoExcerpt"></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </script>
      </div>
      <div class="CoveoPager"></div>
      <div class="CoveoResultsPerPage"></div>
    </div>
  </div>
</div>
