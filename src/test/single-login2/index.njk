		/*
		 * login via AJAX
		 */
		(function initLogin () {

			"use strict";

			var loginForm;


			/**
			 * form url encode inputs for sending over XHR
			 * @arg {Object} form - the form element
			 * @returns {String} the form-urlencoded parameters
			 */
			function formURLEncode (form) {

				var data, i, input, inputs, l;

				// get all the form inputs
				inputs = form.querySelectorAll("input, select, textarea, button");
				l = inputs.length;
				data = [];
				for (i = 0; i < l; i += 1) {

					input = inputs.item(i);
					if (input.name && input.value) {

						data.push(
							encodeURIComponent(input.name) + "=" +
							encodeURIComponent(input.value)
						);

					}

				}

				return data.join("&");

			}


			/**
			 * reset login form after failed login attempt
			 * @arg {Event} event - the form submit event
			 * @returns {null} undefined
			 */
			function resetForm (event) {

				// clear Password field
				event.target.UserPasswd.value = "";

				// enable submit button and reset button label
				event.target._form_action1.disabled = false;
				event.target._form_action.value = "Please wait...";
				event.target._form_action1.value = "Try Again";

			}

			/**
			 * handle error responses from XHR
			 * @this {XMLHttpRequest}
			 * @arg {Event} event - the form submit event
			 * @arg {String} error - the error message
			 * @returns {null} undefined
			 */
			function handleErrorResponse (event, error) {

				var div, msg, template;

				if (!error) {

					// set error message to response status
					msg = [
						"Failed to load resource: ",
						"the server responded with a status of ",
						this.status,
						" (" + this.statusText + ")"
					].join("");

				}

				// create HTML template for error message
				template = "<h5>Error!</h5><p></p>";

				if (error.toString) {

					// ensure msg is a string
					msg = error.toString();

				} else {

					msg = "An unknown error occurred.";

				}

				// wrap last two words in error message with span to prevent wrapping
				msg = msg.replace(
					/\s+(\d+\s+)?([a-zA-Z0-9_\-()]+\s+[a-zA-Z0-9_\-()]+[.?!]?)$/,
					" <span style=\"white-space: nowrap\">$1$2</span>"
				);

				// insert or update the error message in the DOM
				div = document.querySelector(".qualys-client-login-error");
				if (div) {

					div.innerHTML = template.replace("", msg);

				} else {

					div = document.createElement("div");
					div.className = "qualys-client-login-error";
					div.innerHTML = template.replace("", msg);

					// make element focus manageable for Section 508
					div.setAttribute("tabindex", "-1");
					event.target.parentNode.insertBefore(div, event.target);

				}

				// send focus to error message for section 508
				div.focus();
				resetForm(event);

			}

			/**
			 * handle response from backend
			 * @this {XMLHttpRequest}
			 * @arg {Event} event - event
			 * @returns {null} undefined
			 */
			function handleResponse (event) {

				var error, response;

				// handle response
				if (this.readyState === 4) {

					if (this.status >= 200 && this.status < 300) {

						// Success
						try {

							response = JSON.parse(this.responseText);

							if (response.status && response.status === "true") {

								window.location = response.response.redirect;

							} else {

								error = response.response.msg;

							}

						} catch (err) {

							// Error, invalid JSON
							console.error && console.error(err);

							// give the customer a more friendly error
							error = "Service Unavailable";

						}

					} else {

						// Error, response was not 2xx
						// set error message to response status
						var err = [
							"Failed to load resource: ",
							"the server responded with a status of ",
							this.status,
							" (" + this.statusText + ")"
						].join("");
						console.error && console.error(err);

						// give the customer a more friendly error
						error = "Service Unavailable";
					}

				}

				// display error to visitor
				if (error) {

					handleErrorResponse.call(this, event, error);

				}

			}


			/**
			 * handle the form submit event
			 * @arg {Object} event - the form submit event
			 * @returns {Boolean} prevent default
			 */
			function handleLoginFormSubmit (event) {

				var data, div, request, url;

				// stop submission
				event.preventDefault();

				// existing code from the onclick for the submit button
				event.target._form_action1.disabled = true;
				event.target._form_action.value = "Login";
				event.target._form_action1.value = "Please wait...";

				// clear any previous error messages
				div = document.querySelector(".qualys-client-login-error");
				if (div) {

					div.parentElement.removeChild(div);

				}

				// get url from the form action attribute
				url = event.target.getAttribute("action");

				// generate string of form field parameters, url encoded
				data = formURLEncode(event.target);

				// POST JSON to backend and await response
				request = new XMLHttpRequest();
				if (request.timeout) {

					request.timeout = 16000;

				}
				request.open("POST", url, true);
				request.setRequestHeader(
					"Content-Type",
					"application/x-www-form-urlencoded; charset=UTF-8"
				);
				request.setRequestHeader("X-Requested-With", "XMLHttpRequest");
				request.onload = handleResponse.bind(request, event);
				request.onerror = handleErrorResponse.bind(request, event);
				request.send(data);

				return false;

			}

			loginForm = document.querySelector("form[name=\"myform\"]");
			if (loginForm.addEventListener) {

				loginForm.addEventListener("submit", handleLoginFormSubmit, false);

			}

		}());
	</script>
