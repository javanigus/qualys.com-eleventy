						if (version < minimum) {
							deprecated();
						}
					}
				}
			}

			// iterate over unsupported clients
			var client, minimum;
			for (client in clients.unsupported) {
				if (clients.unsupported.hasOwnProperty(client)) {
					if (report.browser.name === client) {
						minimum = clients.unsupported[client];
						if (version < minimum) {
							// special logic for IE
							if (client === "Internet Explorer") {
								if (version === 9) {
									// os version must be Vista SP2
									// or Server 2008 the user agent does not
									// provide service packs and
									// Server 2008 is the same as Vista
									if (report.os && report.os.version === "Vista") {
										deprecated();
									} else {
										unsupported();
									}
								} else if (version === 10) {
									// os version must be Server 2012
									// which is Windows 8 in the user agent
									if (report.os && report.os.version === "8") {
										deprecated();
									} else {
										unsupported();
									}
								} else {
									unsupported();
								}
							} else {
								unsupported();
							}
						}
					}
				}
			}

			// IE must be 6.0 and greater
			// 10 and lower are deprecated
			// 10 is allowed on Server 2012
			// 9 is allowed on Vista SP2 and Server 2008

			// Netscape Navigator must be 8.0 and greater
			// discontinued, all versions are deprecated

			// Mozilla Firefox must be 2.0 and greater
			// less than previous version is outdated (current is 43)

			// Safari must be 2.0.4 and greater
			// less than previous version is outdated (current is 9)

			// Chrome must be any version
			// less than previous version is outdated (current is 48)

			// Edge must be any version
			// less than previous version is outdated (current is 13)

			/*
			 * mark JS support
			 * send visitors to an error page unless cookies are enabled
			 * change class name on HTML element
			 * set client focus to the username field
			 */
			removeClassName(document.documentElement, "no-js");
			addClassName(document.documentElement, "yes-js");

			if ((report && report.cookies) || (!report && navigator.cookieEnabled)) {
				removeClassName(document.documentElement, "no-cookies");
				addClassName(document.documentElement, "yes-cookies");
			}

			document.forms[0].elements.UserLogin.focus();

		}());

		/*
		 * add timezone, timezone abbr, timezone offset, and is DST
		 * as hidden input on form
		 */
		(function addTimeZone () {

			"use strict";

			var now, timeZone;

			/**
			 * helper function to add hidden input elements to the form
			 * @param {string} name The name of the field
			 * @param {string} value The value of the field
			 * @return {undefined}
			 */
			function addHiddenInputToForm (name, value) {

				var input;

				input = document.createElement("input");
				input.type = "hidden";
				input.name = name;
				input.value = value;

				document.forms[0].appendChild(input);

			}

			/**
			 * getTimezone function from Ext 3.4.1.1
			 * @param {Date} date A date object
			 * @return {String} The time zone abbreviation
			 */
			function getTimezoneAbbr (date) {

				return date.toString().replace(/^.* (?:\((.*)\)|([A-Z]{1,4})(?:[\-+][0-9]{4})?(?: -?\d+)?)$/, "$1$2").replace(/[^A-Z]/g, "");

			}

			/**
			 * isDST function from Ext 3.4.1.1
			 * fixed to work in the southern hemisphere as well as the northern
			 * @param {Date} date A date object
			 * @return {Boolean} if this date is observing daylight savings time
			 */
			function isDST (date) {

				var januaryOffset, julyOffset;

				januaryOffset = new Date(date.getFullYear(), 0).getTimezoneOffset();
				julyOffset = new Date(date.getFullYear(), 6).getTimezoneOffset();
				if (julyOffset < januaryOffset) {

					// daylight savings time is observed in July
					return julyOffset === date.getTimezoneOffset();

				} else if (januaryOffset < julyOffset) {

					// daylight savings time is observed in January
					return januaryOffset === date.getTimezoneOffset();

				}

				// time zone does not observe daylight savings time
				return false;

			}

			/**
			 * get time zone value from the ECMAScript Internationalization API
			 * @return {String} The IANA zone info key, e.g. America/Los_Angeles
			 */
			function getTimeZone () {

				var intlOptions;

				if (window.Intl && Intl.DateTimeFormat) {

					intlOptions = (new Intl.DateTimeFormat()).resolvedOptions();
					if (intlOptions && intlOptions.timeZone) {

						return intlOptions.timeZone;

					}

				}

				return "";

			}

			timeZone = getTimeZone();
			if (timeZone) {

				addHiddenInputToForm("timezone", timeZone);

			}

			now = new Date();
			addHiddenInputToForm("gmtoffset", now.getTimezoneOffset());
			addHiddenInputToForm("timezone_abbr", getTimezoneAbbr(now));
			addHiddenInputToForm("is_dst", Number(isDST(now)));

